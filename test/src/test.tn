import io;

fn main() {
    let mut result;
    
    result = 1;
    if result != 1 {
        print("FAILED: return 1\n");
        exit(1);
    }
    
    result = 5 + 3;
    if result != 8 {
        print("FAILED: 5+3\n");
        exit(1);
    }
    
    result = 10 - 4;
    if result != 6 {
        print("FAILED: 10-4\n");
        exit(1);
    }
    
    result = 6 * 7;
    if result != 42 {
        print("FAILED: 6*7\n");
        exit(1);
    }
    
    result = 20 / 4;
    if result != 5 {
        print("FAILED: 20/4\n");
        exit(1);
    }
    
    result = 10 % 3;
    if result != 1 {
        print("FAILED: 10%3\n");
        exit(1);
    }
    
    result = 2 + 3 * 4;
    if result != 14 {
        print("FAILED: 2+3*4\n");
        exit(1);
    }
    
    result = (2 + 3) * 4;
    if result != 20 {
        print("FAILED: (2+3)*4\n");
        exit(1);
    }
    
    result = 2 == 2;
    if result != 1 {
        print("FAILED: 2==2\n");
        exit(1);
    }
    
    result = 2 != 3;
    if result != 1 {
        print("FAILED: 2!=3\n");
        exit(1);
    }
    
    result = 2 < 3;
    if result != 1 {
        print("FAILED: 2<3\n");
        exit(1);
    }
    
    result = 3 >> 1;
    if result != 1 {
        print("FAILED: 3>>1\n");
        exit(1);
    }
    
    result = 4 << 1;
    if result != 8 {
        print("FAILED: 4<<1\n");
        exit(1);
    }
    
    result = 1 & 3;
    if result != 1 {
        print("FAILED: 1&3\n");
        exit(1);
    }
    
    result = 5 | 2;
    if result != 7 {
        print("FAILED: 5|2\n");
        exit(1);
    }
    
    result = 1 || 0;
    if result != 1 {
        print("FAILED: 1||0\n");
        exit(1);
    }
    
    result = 1 && 1;
    if result != 1 {
        print("FAILED: 1&&1\n");
        exit(1);
    }
    
    let mut d;
    d = 0;
    while d < 5 {
        d = d + 1;
    }
    if d != 5 {
        print("FAILED: while loop\n");
        exit(1);
    }
    
    print("All tests passed!\n");
    exit(0);
}
